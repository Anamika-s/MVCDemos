@model IEnumerable<WebApplication14.Models.Student>
@{
    ViewBag.Title = "Index1";
}

<h2> List of Studnets </h2>

<table style="width:100%; height:80%; background-color:pink">
    <tr>
        <th> ID </th>
        <th> Name </th>
        <th> Marks </th>
        <th> Batch Code </th>
    </tr>

    @{ foreach (var temp in Model)
        {
            <tr>
                <td> @temp.Id </td>
                <td>   @temp.Name </td>
                <td> @temp.Marks</td>
                <td> @temp.BatchCode </td>
            </tr>
        }

    }
</table>
